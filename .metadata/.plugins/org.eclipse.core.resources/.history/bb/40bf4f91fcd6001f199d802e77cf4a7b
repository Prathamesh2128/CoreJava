package Hello;

public class Demo {

	public static void main(String[] args) {

		String name = "swiss";

		char arr[] = name.toCharArray();

		for (int i = 0; i < arr.length; i++) {
			for (int j = 0; i < arr.length+1; j++) {
				System.out.println(arr[j]);
			}
			System.out.println(arr[i]);
		}

	}

}
